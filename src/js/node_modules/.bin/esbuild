#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/www/fre-embed/src/js/node_modules/.pnpm/esbuild@0.15.16/node_modules/esbuild/bin/node_modules:/mnt/d/www/fre-embed/src/js/node_modules/.pnpm/esbuild@0.15.16/node_modules/esbuild/node_modules:/mnt/d/www/fre-embed/src/js/node_modules/.pnpm/esbuild@0.15.16/node_modules:/mnt/d/www/fre-embed/src/js/node_modules/.pnpm/node_modules:/mnt/d/www/fre-embed/src/js/node_modules:/mnt/d/www/fre-embed/src/node_modules:/mnt/d/www/fre-embed/node_modules:/mnt/d/www/node_modules:/mnt/d/node_modules:/mnt/d/www/fre-embed/src/JS/node_modules/esbuild/bin/node_modules:/mnt/d/www/fre-embed/src/JS/node_modules/esbuild/node_modules:/mnt/d/www/fre-embed/src/JS/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/d/www/fre-embed/src/js/node_modules/.pnpm/esbuild@0.15.16/node_modules/esbuild/bin/node_modules:/mnt/d/www/fre-embed/src/js/node_modules/.pnpm/esbuild@0.15.16/node_modules/esbuild/node_modules:/mnt/d/www/fre-embed/src/js/node_modules/.pnpm/esbuild@0.15.16/node_modules:/mnt/d/www/fre-embed/src/js/node_modules/.pnpm/node_modules:/mnt/d/www/fre-embed/src/js/node_modules:/mnt/d/www/fre-embed/src/node_modules:/mnt/d/www/fre-embed/node_modules:/mnt/d/www/node_modules:/mnt/d/node_modules:/mnt/d/www/fre-embed/src/JS/node_modules/esbuild/bin/node_modules:/mnt/d/www/fre-embed/src/JS/node_modules/esbuild/node_modules:/mnt/d/www/fre-embed/src/JS/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../esbuild/bin/esbuild" "$@"
else
  exec node  "$basedir/../esbuild/bin/esbuild" "$@"
fi
